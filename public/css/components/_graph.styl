.graph
  margin: 0

path
  stroke: white
  fill: white
  stroke-width: 0

text
  font: 10px sans-serif

.axis path, .axis line
  fill: none
  stroke: #000
  shape-rendering: crispEdges

.x.axis path
  display: none

.line
  fill: none
  stroke: maroon
  stroke-width: 2px

.line.above
  stroke: steelblue

.focus circle
  fill: steelblue
  stroke: steelblue

.label
  fill: steelblue
  font-size: 12px
  font-family: Georgia

.overlay
  fill: none
  pointer-events: all
